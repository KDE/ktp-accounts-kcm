<?xml version="1.0" encoding="UTF-8" ?>
<service id="ktp-haze-sametime-im">
    <type>IM</type>
    <!-- IBM Sametime Chat - Service name being displayed as a checkbox label to enable/disable this service -->
    <_name>Chat</_name>
    <icon>im-user</icon>
    <provider>ktp-haze-sametime</provider>
    <translations>kaccounts-providers</translations>

    <template>
        <group name="telepathy">
            <setting name="manager">haze</setting>
            <setting name="protocol">sametime</setting>
        </group>
        <group name="auth">
            <setting name="method">password</setting>
            <setting name="mechanism">password</setting>
        </group>
    </template>
</service>
